{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_authors}"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "${mod_git_repo}"
	},
	"license": "${mod_license}",
	"icon": "assets/${mod_id}/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"${mod_group}.fabric.Imagemod"
		],
		"client": [
			"${mod_group}.fabric.ImagemodClient"
		]
	},
	"mixins": [
		"${mod_id}.mixins.json",
		{
			"config": "${mod_id}.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": "${fabric_loader_version_range}",
		"minecraft": "${fabric_minecraft_version_range}",
		"java": ">=${java_version}",
		"fabric-api": "${fabric_api_version_range}"
	},
	"suggests": {}
}