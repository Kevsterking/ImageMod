{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_authors}"
	],
	"contact": {
		"homepage": "${mod_git_repo}",
		"sources": "${mod_git_repo}"
	},
	"license": "${mod_license}",
	"icon": "icon.png",
	"environment": "*",
	"entrypoints": {
		"client": [
			"${mod_group}.fabric.ImageModClient"
		]
	},
	"depends": {
		"fabricloader": "${fabric_loader_version_range}",
		"minecraft": "${fabric_minecraft_version_range}",
		"java": ">=${java_version}",
		"fabric-api": "${fabric_api_version_range}"
	},
	"suggests": {}
}